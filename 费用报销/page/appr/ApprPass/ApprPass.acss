@import "/component/select/select.acss";

.pageBody{
  width: 100%;
}
.remark{
  width: 100%;
  height: 600rpx;
}
.remarkVal{
  width:92%;
  height: 540rpx;
  margin: 30rpx;
  font-size: 0.3rem;
  text-align: left;
  font-family: PingFang-SC-Medium;
  color: #575b60;
  font-weight: 500;
  line-height: 34rpx;
}
.Copier{
  width: 89.34%;
  height: 130rpx;
  border-top: 30rpx solid #F6F6F6;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  padding: 0 5.33%;
}
.page-item-titles{
  text-align: left;
  line-height: 36rpx;
  padding-top: 14rpx;
}
.page-titles-title{
  font-size:32rpx;
  font-family:PingFang-SC-Medium;
  font-weight:500;
  color: #000000;
}
.page-titles-por{
  font-size:22rpx;
  font-family:PingFang-SC-Medium;
  font-weight:500;
  color: #A4A4A4;
}
.addCC{
  width: 70rpx;
  height: 70rpx;
}
.page-item-imgCC{
  flex: 1;
  display: flex;
  align-items: center;
  justify-content: flex-end;
  flex-direction: row;
}
.imgCC-all{
  display: flex;
  align-items: center;
  flex-direction: row;
}
.imgCC-allL{
  display: flex;
  flex-direction: column;
  align-items: center;
  margin-top: 20rpx;
  position: relative;
}
.imgCC-all-img{
  width: 70rpx;
  height: 70rpx;
  border-radius: 50%;
}
.headName{
  width: 70rpx;
  height: 70rpx;
  font-size: 22rpx;
  background-color: #399EFA;
  color: #FFFFFF;
  text-align: center;
  line-height: 70rpx;
  border-radius: 50%;
}
.imgCC-all-title{
  font-size: 20rpx;
  font-family: PingFang-SC-Regular;
  font-weight: 400;
  color: #A4A4A4;
  padding-top: 9rpx;
}
.imgCC-cancel{
  width: 30rpx;
  height: 30rpx;
  position: absolute;
  top: -8rpx;
  right: -8rpx;
}
.imgCC-allR{
  font-size: 28rpx;
  font-family: PingFang-SC-Regular;
  font-weight: 500;
  color: #A4A4A4;
  padding: 0 12rpx;
}
.imgCC-items,.imgCC-item{
  display: flex;
  flex-direction: row;
  align-items: center;
}
.btn-box{
  width: 100%;
  height: 130rpx;
  background-color: white;
  position: fixed;
  bottom: 0rpx;
}
.btn{
  width: 86.67%;
  height: 90rpx;
  background-color: #399EFA;
  border-radius: 4rpx;
  font-size: 34rpx;
  color: #FFFFFF;
  text-align: center;
  line-height: 90rpx;
  margin: 20rpx 6.665%;
}
.ApprPeo{
  width: 89.34%;
  height: 100rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  padding: 0 5.33%;
  border-bottom: 2rpx solid #F7F7F7;
  border-top: 10rpx solid #F7F7F7;
}
.ApprPeo-title{
  font-size: 32rpx;
  color: #000000;
}
.ApprPeo-val{
  flex: 1;
  width: 100%;
  text-align: right;
}
.ApprPeo-input{
  width: 100%;
  height: 90rpx;
  text-align: right;
  font-size: 32rpx;
  line-height: 90rpx;
  background-color: none;
}
.ApprPeo-right-select{
  width: 20rpx;
  height: 12rpx;
  margin-left: 10rpx;
}
/* 下拉框样式 */
.page-select{
  padding-bottom: 130rpx;
}